{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://prakharsingh.github.io",
  "author": "prakharsingh007@gmail.com",
  "main": "index.tsx",
  "scripts": {
    "build": "PUBLIC_URL=https://prakharsingh.github.io react-scripts build",
    "predeploy": "npm run build",
    "deploy": "npx gh-pages -b master -d build",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react-scripts": "3.4.0",
    "typescript": "^3.8.3"
  }
}
